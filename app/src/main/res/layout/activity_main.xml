<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/mainBackground"
        android:layout_width="884dp"
        android:layout_height="458dp"
        android:onClick="shareInfoToMain"
        app:srcCompat="@drawable/LandscapeBackground"
        tools:layout_editor_absoluteX="-77dp"
        tools:layout_editor_absoluteY="-24dp" />

    <ImageView
        android:id="@+id/title"
        android:layout_width="516dp"
        android:layout_height="132dp"
        app:srcCompat="@drawable/Logo"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/psyduckImage"
        android:layout_width="87dp"
        android:layout_height="154dp"
        android:onClick="startGame"
        app:srcCompat="@drawable/psyduck"
        tools:layout_editor_absoluteX="322dp"
        tools:layout_editor_absoluteY="195dp" />

</androidx.constraintlayout.widget.ConstraintLayout>